        <activity android:name="com.csform.android.uiapptemplate.DialogShopActivity" />
        <activity android:name="com.csform.android.uiapptemplate.DialogSocialActivity" />
        <activity android:name="com.csform.android.uiapptemplate.DialogUniversalWarningActivity" />
        <activity
            android:name="com.csform.android.uiapptemplate.WizardTravelActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTabsTheme"
            android:windowSoftInputMode="adjustResize|stateHidden" />
