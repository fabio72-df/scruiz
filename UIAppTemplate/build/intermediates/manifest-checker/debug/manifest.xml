            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="com.csform.android.uiapptemplate.WizardSocialActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTabsTheme"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity
            android:name="com.csform.android.uiapptemplate.WizardUniversalActivity"
