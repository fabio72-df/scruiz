        <activity android:name="com.csform.android.uiapptemplate.GoogleCardsShopActivity" />
        <activity android:name="com.csform.android.uiapptemplate.GoogleCardsSocialActivity" />
        <activity android:name="com.csform.android.uiapptemplate.ExpandableListViewActivity" />
        <activity android:name="com.csform.android.uiapptemplate.ExpandableMediaListViewActivity" />
        <activity android:name="com.csform.android.uiapptemplate.ExpandableSocialListViewActivity" />
        <activity android:name="com.csform.android.uiapptemplate.ExpandableTravelListViewActivity" />
        <activity android:name="com.csform.android.uiapptemplate.ExpandableShopListViewActivity" />
